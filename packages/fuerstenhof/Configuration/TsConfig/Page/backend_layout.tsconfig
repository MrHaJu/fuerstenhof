mod.web_layout.BackendLayouts {
    layoutwithheader {
        title = Layout mit Header
        #icon = EXT:example_extension/Resources/Public/Images/BackendLayouts/default.gif
        config {
            backend_layout {
                colCount = 1
                rowCount = 2
                rows {
                    1 {
                        columns {
                            1 {
                                name = Header
                                colPos = 1
                            }
                        }
                    }
                    2 {
                        columns {
                            1 {
                                name = Normal
                                colPos = 0
                            }
                        }
                    }
                }
            }
        }
    }
}

mod.web_layout.BackendLayouts {
    layoutwithoutheader {
        title = Layout ohne Header
        #icon = EXT:example_extension/Resources/Public/Images/BackendLayouts/default.gif
        config {
            backend_layout {
                colCount = 1
                rowCount = 2
                rows {
                    1 {
                        columns {
                            1 {
                                name = Normal
                                colPos = 0
                            }
                        }
                    }
                }
            }
        }
    }
}
